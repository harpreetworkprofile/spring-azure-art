{
  "scriptFile" : "../azure-spring-art-1.0-SNAPSHOT.jar",
  "entryPoint" : "com.example.hello.HelloHandler.execute",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "request",
    "route" : "service/hello",
    "methods" : [ "GET", "POST" ],
    "authLevel" : "ANONYMOUS"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ]
}